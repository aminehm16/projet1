#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main() {
    int t[5];
    int i, s = 0; 

    printf("Donnez des valeurs pour le tableau\n ");
    
    for (i = 0; i < 5; i++) {
        scanf("%d", &t[i]); 
        s = s + t[i];
    }

    printf("La somme des éléments du tableau est %d\n", s); 
    
    return 0;
}
